<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="menu">
        <item name="android:background">@drawable/click_nn</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">2.0dip</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="menu_tab"/>

    <style name="default_linerlayout">
        <item name="android:background">#00000000</item>
        <item name="android:padding">3.0dip</item>
        <item name="android:gravity">top|right</item>
    </style>

    <style name="default_dian">
        <item name="android:background">@drawable/xiaohong</item>
